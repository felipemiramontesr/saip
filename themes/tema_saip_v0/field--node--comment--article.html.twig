<div class="comment_article">
  <section{{ attributes }}>
    {% if comments and not label_hidden %}
      {{ title_prefix }}
      <h2{{ title_attributes }}>{{ label }}</h2>
      {{ title_suffix }}
    {% endif %}

    {{ comments }}

    {% if comment_form %}
      <h2{{ content_attributes }}>{{ ''|t }}</h2>
      {{ comment_form }}
    {% endif %}
  </section>
</div>
